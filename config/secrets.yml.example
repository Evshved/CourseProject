development:
  twitter_consumer_key: 'your secret key'
  twitter_consumer_secret: 'your secret key'
  facebook_consumer_key: 'your secret key'
  facebook_consumer_secret: 'your secret key'
  vk_consumer_key: 'your secret key'
  vk_consumer_secret: 'your secret key'
  secret_key_base: your secret key

  cloud_name: your_cloud_name
  api_key: 'your secret key'
  api_secret: your secret key
  enhance_image_tag: true
  static_image_support: false

test:
  secret_key_base: your secret key
  cloud_name: your_cloud_name
  api_key: 'your secret key'
  api_secret: your secret key
  enhance_image_tag: true
  static_image_support: false

# Do not keep production secrets in the repository,
# instead read values from the environment.
production:
  secret_key_base: <%= ENV["SECRET_KEY_BASE"] %>
  cloud_name: your_cloud_name
  api_key: 'your secret key'
  api_secret: your secret key
  enhance_image_tag: true
  static_image_support: true
